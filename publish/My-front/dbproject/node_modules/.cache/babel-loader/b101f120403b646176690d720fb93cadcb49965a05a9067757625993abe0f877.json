t>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"addPolicemenID\"\r\n                placeholder=\"请输入警员编号\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\r\n                clearable\r\n                maxlength=\"7\"\r\n                show-word-limit\r\n              />\r\n            </td>\r\n            <td></td>\r\n          </tr>\r\n        </table>\r\n\r\n        <div class=\"seaButton\">\r\n          <el-button type=\"primary\" @click=\"addAlarmInfo\"\r\n            >上传接警记录</el-button\r\n          >\r\n        </div>\r\n      </div>\r\n    </el-tab-pane>\r\n\r\n    <!-- 删除接警记录栏 -->\r\n    <el-tab-pane label=\"删除接警记录\" name=\"3\">\r\n      <div style=\"text-align: center\">\r\n        <table style=\"display: inline-block; text-align: left\">\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">接警记录编号:</el-text>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"delAlarmID\"\r\n                placeholder=\"请输入接警记录编号\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\r\n                clearable\r\n                maxlength=\"7\"\r\n                show-word-limit\r\n              />\r\n            </td>\r\n            <td>\r\n              <el-button\r\n                type=\"primary\"\r\n                @click=\"delAlarmInfo\"\r\n                style=\"margin-left: 20px\"\r\n                >删除接警记录</el-button\r\n              >\r\n            </td>\r\n          </tr>\r\n        </table>\r\n      </div>\r\n    </el-tab-pane>\r\n\r\n    <!-- 关联案件栏 -->\r\n    <el-tab-pane label=\"添加/修改关联案件\" name=\"4\">\r\n      <div style=\"text-align: center\">\r\n        <table style=\"display: inline-block; text-align: left\">\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">涉及案件编号:</el-text>\r\n              <el-input\r\n                v-model=\"relCaseID\"\r\n                placeholder=\"请输入案件编号\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\r\n                clearable\r\n                maxlength=\"7\"\r\n                show-word-limit\r\n                style=\"width: 220px; margin-left: 10px\"\r\n              >\r\n                <template #prepend>\r\n                  <div class=\"qianru\">{{ type4 }}</div>\r\n                </template></el-input\r\n              >\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">案件类型:</el-text>\r\n              <el-select\r\n                class=\"inputBox\"\r\n                v-model=\"relCaseType\"\r\n                style=\"margin-left: 10px; width: 200px\"\r\n                placeholder=\"请选择\"\r\n              >\r\n                <el-option label=\"强奸\" value=\"强奸\"></el-option>\r\n                <el-option label=\"抢劫\" value=\"抢劫\"></el-option>\r\n                <el-option label=\"故意伤害\" value=\"故意伤害\"></el-option>\r\n                <el-option label=\"盗窃\" value=\"盗窃\"></el-option>\r\n                <el-option label=\"诈骗\" value=\"诈骗\"></el-option>\r\n                <el-option label=\"谋杀\" value=\"谋杀\"></el-option>\r\n              </el-select>\r\n            </td>\r\n          </tr>\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">接警记录编号:</el-text>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"relAlarmID\"\r\n                placeholder=\"请输入接警记录编号\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\r\n                clearable\r\n                maxlength=\"7\"\r\n                show-word-limit\r\n              />\r\n            </td>\r\n            <td></td>\r\n          </tr>\r\n        </table>\r\n        <div class=\"seaButton\">\r\n          <el-button type=\"primary\" @click=\"relAlarmInfo\">关联案件</el-button>\r\n        </div>\r\n      </div>\r\n    </el-ta