{"ast":null,"code":"import axios from \"../../api/request\";\nimport { ElMessage } from 'element-plus';\nexport default {\n  data() {\n    return {\n      repeatOffenderInfo: \"\",\n      roName: \"\",\n      isDetail: false,\n      roID: \"\",\n      roSex: \"全部\",\n      roAddress: \"\",\n      caseRecord: \"\"\n    };\n  },\n  methods: {\n    handleMouseMove(event) {\n      const x = event.pageX - event.target.offsetLeft;\n      const y = event.pageY - event.target.offsetTop;\n      event.target.style.setProperty(\"--x\", `${x}px`);\n      event.target.style.setProperty(\"--y\", `${y}px`);\n    },\n    async getRepeatOffenderInfo() {\n      try {\n        const response = await axios.get(\"/api/keyIndividualsStatistics/repeatOffenderInfoStatistics\");\n        this.repeatOffenderInfo = response.data;\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"获取重犯列表失败！\",\n          type: 'error',\n          duration: 5000\n        });\n      }\n    },\n    goBack() {\n      if (this.isDetail) {\n        this.isDetail = false;\n      }\n    },\n    async getDetail(index, man) {\n      window.scrollTo({\n        top: 0,\n        behavior: 'smooth' // 可选的平滑滚动效果\n      });\n\n      this.isDetail = true;\n      try {\n        const response = await axios.get(\"/api/keyIndividualsStatistics/repeatOffenderCaseStatistics\", {\n          params: {\n            id: man[\"身份证号\"]\n          }\n        });\n        for (let i = 0; i < response.data.length; i++) {\n          if (response.data[i][\"案件等级\"] == \"0\") response.data[i][\"案件等级\"] = \"一般\";else if (response.data[i][\"案件等级\"] == \"1\") response.data[i][\"案件等级\"] = \"较大\";else if (response.data[i][\"案件等级\"] == \"2\") response.data[i][\"案件等级\"] = \"重大\";else if (response.data[i][\"案件等级\"] == \"3\") response.data[i][\"案件等级\"] = \"特别重大\";\n        }\n        this.caseRecord = response.data;\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"获取案件信息失败！\",\n          type: 'error',\n          duration: 5000\n        });\n      }\n    },\n    async filter() {\n      try {\n        const response = await axios.get(`/api/keyIndividualsStatistics/repeatOffenderFilterStatistics?ID=${encodeURIComponent(this.roID)}&name=${encodeURIComponent(this.roName)}&sex=${encodeURIComponent(this.roSex)}&address=${encodeURIComponent(this.roAddress)}`);\n        this.repeatOffenderInfo = response.data;\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"信息失败！\",\n          type: 'error',\n          duration: 5000\n        });\n      }\n    }\n  },\n  async created() {\n    await this.getRepeatOffenderInfo();\n  },\n  watch: {\n    async roID() {\n      await this.filter();\n    },\n    async roName() {\n      await this.filter();\n    },\n    async roSex() {\n      await this.filter();\n    },\n    async roAddress() {\n      await this.filter();\n    }\n  }\n};","map":{"version":3,"names":["axios","ElMessage","data","repeatOffenderInfo","roName","isDetail","roID","roSex","roAddress","caseRecord","methods","handleMouseMove","event","x","pageX","target","offsetLeft","y","pageY","offsetTop","style","setProperty","getRepeatOffenderInfo","response","get","error","showClose","message","type","duration","goBack","getDetail","index","man","window","scrollTo","top","behavior","params","id","i","length","filter","encodeURIComponent","created","watch"],"sources":["G:\\publish\\My-front\\dbproject\\src\\components\\CaseManagement\\KeyIndividualsManagement.vue"],"sourcesContent":["<template>\n  <el-header class=\"sub-header\" @mousemove=\"handleMouseMove\">\n    <div>&nbsp;&nbsp;案件管理&nbsp;>&nbsp;重点人员统计</div>\n  </el-header>\n\n  <div class=\"main\">\n    <!-- 表格 -->\n    <el-table v-if=\"!isDetail\" :data=\"repeatOffenderInfo\" height=\"450\" @wheel.passive.stop stripe style=\"width: 100%\">\n      <el-table-column label=\"身份证号码\" prop=\"身份证号\" sortable width=\"180px\" />\n      <el-table-column label=\"姓名\" prop=\"姓名\" sortable width=\"100px\" />\n      <el-table-column label=\"性别\" prop=\"性别\" sortable width=\"80px\" />\n      <el-table-column label=\"住址\" prop=\"住址\" sortable />\n      <el-table-column align=\"right\" width=\"300\">\n        <template #header>\n          <table>\n            <tr>\n              <td><el-text type=\"primary\" style=\"display: inline-block;\">筛选身份证号码：</el-text></td>\n              <td><el-input v-model=\"roID\" size=\"small\" placeholder=\"请输入身份证号码\"\n                  style=\"display: inline-block; width: 140px; height: 25px\" maxlength=\"18\"\n                  oninput=\"value=value.replace(/[^\\d.]/g,'')\" /></td>\n            </tr>\n            <tr>\n              <td><el-text type=\"primary\" style=\"display: inline-block;\">筛选姓名：</el-text></td>\n              <td><el-input v-model=\"roName\" size=\"small\" placeholder=\"请输入姓名\"\n                  style=\"display: inline-block; width: 140px; height: 25px\" /></td>\n            </tr>\n            <tr>\n              <td><el-text type=\"primary\" style=\"display: inline-block;\">筛选性别：</el-text></td>\n              <td>\n                <el-select v-model=\"roSex\" placeholder=\"请选择\" size=\"small\"\n                  style=\"display: inline-block; width: 140px; height: 25px\">\n                  <el-option label=\"全部性别\" value=\"全部\" />\n                  <el-option label=\"男\" value=\"M\" />\n                  <el-option label=\"女\" value=\"F\" />\n                </el-select>\n              </td>\n            </tr>\n            <tr>\n              <td><el-text type=\"primary\" style=\"display: inline-block;\">筛选住址：</el-text></td>\n              <td><el-input v-model=\"roAddress\" size=\"small\" placeholder=\"请输入住址\"\n                  style=\"display: inline-block; width: 140px; height: 25px\" /></td>\n            </tr>\n          </table>\n        </template>\n        <template #default=\"scope\">\n          <el-button size=\"small\" type=\"primary\" @click=\"getDetail(scope.$index, scope.row)\">详情</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <el-table v-if=\"isDetail\" :data=\"caseRecord\" height=\"450\" @wheel.passive.stop stripe style=\"width: 100%\">\n      <el-table-column label=\"案件编号\" prop=\"案件编号\" sortable width=\"110px\" />\n      <el-table-column label=\"案件类型\" prop=\"案件类型\" sortable width=\"110px\" />\n      <el-table-column label=\"案件状态\" prop=\"案件状态\" sortable width=\"110px\" />\n      <el-table-column label=\"登记时间\" prop=\"登记时间\" sortable />\n      <el-table-column label=\"案发地点\" prop=\"案发地点\" sortable />\n      <el-table-column label=\"案件等级\" prop=\"案件等级\" sortable width=\"110px\" />\n      <el-table-column width=\"120px\">\n        <template #header>\n          <el-button type=\"primary\" @click=\"goBack\" size=\"small\">返回重犯列表</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n  </div>\n</template>\n\n<script>\nimport axios from \"../../api/request\";\n\nimport { ElMessage } from 'element-plus';\n\nexport default {\n  data() {\n    return {\n      repeatOffenderInfo: \"\",\n      roName: \"\",\n      isDetail: false,\n      roID: \"\",\n      roSex: \"全部\",\n      roAddress: \"\",\n      caseRecord: \"\",\n    };\n  },\n  methods: {\n    handleMouseMove(event) {\n      const x = event.pageX - event.target.offsetLeft;\n      const y = event.pageY - event.target.offsetTop;\n\n      event.target.style.setProperty(\"--x\", `${x}px`);\n      event.target.style.setProperty(\"--y\", `${y}px`);\n    },\n    async getRepeatOffenderInfo() {\n      try {\n        const response = await axios.get(\"/api/keyIndividualsStatistics/repeatOffenderInfoStatistics\");\n        this.repeatOffenderInfo = response.data;\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"获取重犯列表失败！\",\n          type: 'error',\n          duration: 5000,\n        });\n      }\n    },\n    goBack() {\n      if (this.isDetail) {\n        this.isDetail = false;\n      }\n    },\n    async getDetail(index, man) {\n      window.scrollTo({\n        top: 0,\n        behavior: 'smooth' // 可选的平滑滚动效果\n      });\n      this.isDetail = true;\n      try {\n        const response = await axios.get(\"/api/keyIndividualsStatistics/repeatOffenderCaseStatistics\", { params: { id: man[\"身份证号\"] } });\n        for (let i = 0; i < response.data.length; i++) {\n          if (response.data[i][\"案件等级\"] == \"0\")\n            response.data[i][\"案件等级\"] = \"一般\";\n          else if (response.data[i][\"案件等级\"] == \"1\")\n            response.data[i][\"案件等级\"] = \"较大\";\n          else if (response.data[i][\"案件等级\"] == \"2\")\n            response.data[i][\"案件等级\"] = \"重大\";\n          else if (response.data[i][\"案件等级\"] == \"3\")\n            response.data[i][\"案件等级\"] = \"特别重大\";\n        }\n        this.caseRecord = response.data;\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"获取案件信息失败！\",\n          type: 'error',\n          duration: 5000,\n        });\n      }\n    },\n    async filter() {\n      try {\n        const response = await axios.get(`/api/keyIndividualsStatistics/repeatOffenderFilterStatistics?ID=${encodeURIComponent(this.roID)}&name=${encodeURIComponent(this.roName)}&sex=${encodeURIComponent(this.roSex)}&address=${encodeURIComponent(this.roAddress)}`);\n        this.repeatOffenderInfo = response.data;\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"信息失败！\",\n          type: 'error',\n          duration: 5000,\n        });\n      }\n    }\n  },\n  async created() {\n    await this.getRepeatOffenderInfo();\n  },\n  watch: {\n    async roID() {\n      await this.filter();\n    },\n    async roName() {\n      await this.filter();\n    },\n    async roSex() {\n      await this.filter();\n    },\n    async roAddress() {\n      await this.filter();\n    },\n  }\n};\n</script>\n\n<style>\n.sub-header {\n  overflow: hidden;\n  display: flex;\n  position: absolute;\n  top: 70px;\n  left: 199px;\n  width: calc(100% - 199px);\n  height: 7vh;\n  min-height: 40px;\n  align-items: center;\n  /* 文字竖直方向居中对齐 */\n  background-color: #1f2cdf;\n  box-shadow: inset -500px 0px 200px 0px rgba(4, 0, 113, 0.856);\n  color: #ffffff;\n  font-size: 28px;\n}\n\n.sub-header::before {\n  --size: 0;\n  content: '';\n  position: absolute;\n  left: var(--x);\n  top: var(--y);\n  width: var(--size);\n  height: var(--size);\n  background: radial-gradient(circle closest-side, #5a65ff, transparent);\n  transform: translate(-50%, -50%);\n  transition: width .2s ease, height .2s ease;\n}\n\n.sub-header:hover::before {\n  --size: 400px;\n}\n\n.main {\n  margin-top: 10vh;\n}\n</style>"],"mappings":"AAmEA,OAAOA,KAAI,MAAO,mBAAmB;AAErC,SAASC,SAAQ,QAAS,cAAc;AAExC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,kBAAkB,EAAE,EAAE;MACtBC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,KAAK;MACfC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,IAAI;MACXC,SAAS,EAAE,EAAE;MACbC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,eAAeA,CAACC,KAAK,EAAE;MACrB,MAAMC,CAAA,GAAID,KAAK,CAACE,KAAI,GAAIF,KAAK,CAACG,MAAM,CAACC,UAAU;MAC/C,MAAMC,CAAA,GAAIL,KAAK,CAACM,KAAI,GAAIN,KAAK,CAACG,MAAM,CAACI,SAAS;MAE9CP,KAAK,CAACG,MAAM,CAACK,KAAK,CAACC,WAAW,CAAC,KAAK,EAAG,GAAER,CAAE,IAAG,CAAC;MAC/CD,KAAK,CAACG,MAAM,CAACK,KAAK,CAACC,WAAW,CAAC,KAAK,EAAG,GAAEJ,CAAE,IAAG,CAAC;IACjD,CAAC;IACD,MAAMK,qBAAqBA,CAAA,EAAG;MAC5B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMvB,KAAK,CAACwB,GAAG,CAAC,4DAA4D,CAAC;QAC9F,IAAI,CAACrB,kBAAiB,GAAIoB,QAAQ,CAACrB,IAAI;MACzC,EAAE,OAAOuB,KAAK,EAAE;QACd;QACAxB,SAAS,CAAC;UACRyB,SAAS,EAAE,IAAI;UACfC,OAAO,EAAE,WAAW;UACpBC,IAAI,EAAE,OAAO;UACbC,QAAQ,EAAE;QACZ,CAAC,CAAC;MACJ;IACF,CAAC;IACDC,MAAMA,CAAA,EAAG;MACP,IAAI,IAAI,CAACzB,QAAQ,EAAE;QACjB,IAAI,CAACA,QAAO,GAAI,KAAK;MACvB;IACF,CAAC;IACD,MAAM0B,SAASA,CAACC,KAAK,EAAEC,GAAG,EAAE;MAC1BC,MAAM,CAACC,QAAQ,C