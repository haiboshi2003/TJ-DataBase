{"ast":null,"code":"import axios from \"../../api/request\";\nimport { ElMessage } from \"element-plus\";\nexport default {\n  data() {\n    return {\n      anv: \"1\",\n      citizenID: \"\",\n      citizenName: \"\",\n      citizenAddress: \"\",\n      citizenSex: \"\",\n      citizenInfo: [],\n      addCitizenID: \"\",\n      addCitizenName: \"\",\n      addCitizenAddress: \"\",\n      addCitizenSex: \"\",\n      addMotherCitizenID: \"\",\n      addFatherCitizenID: \"\",\n      delCitizenID: \"\",\n      updCitizenID: \"\",\n      updCitizenName: \"\",\n      updCitizenAddress: \"\",\n      updCitizenSex: \"\",\n      updMotherCitizenID: \"\",\n      updFatherCitizenID: \"\"\n    };\n  },\n  methods: {\n    handleMouseMove(event) {\n      const x = event.pageX - event.target.offsetLeft;\n      const y = event.pageY - event.target.offsetTop;\n      event.target.style.setProperty(\"--x\", `${x}px`);\n      event.target.style.setProperty(\"--y\", `${y}px`);\n    },\n    async fetchCitizenInfo() {\n      try {\n        const response = await axios.get(`/api/getCitizenInfo?citizenID=${encodeURIComponent(this.citizenID)}&citizenName=${encodeURIComponent(this.citizenName)}&citizenSex=${encodeURIComponent(this.citizenSex)}&citizenAddress=${encodeURIComponent(this.citizenAddress)}`);\n        this.citizenInfo = response.data.map(item => {\n          if (item.sex === \"M\") {\n            return {\n              ...item,\n              sex: \"男\"\n            };\n          } else if (item.sex === \"F\") {\n            return {\n              ...item,\n              sex: \"女\"\n            };\n          } else {\n            return item;\n          }\n        });\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"获取公民列表失败！\",\n          type: \"error\",\n          duration: 5000\n        });\n      }\n    },\n    async addCitizenInfo() {\n      try {\n        const response = await axios.post(`/api/addCitizen?citizenID=${encodeURIComponent(this.addCitizenID)}&citizenName=${encodeURIComponent(this.addCitizenName)}&citizenSex=${encodeURIComponent(this.addCitizenSex)}&citizenAddress=${encodeURIComponent(this.addCitizenAddress)}&motherID=${encodeURIComponent(this.addMotherCitizenID)}&fatherID=${encodeURIComponent(this.addFatherCitizenID)}`);\n        if (typeof response.data == \"string\") {\n          ElMessage({\n            showClose: true,\n            message: response.data,\n            type: \"warning\",\n            duration: 5000\n          });\n        } else {\n          await this.fetchCitizenInfo();\n          ElMessage({\n            showClose: true,\n            message: \"新增公民成功! \",\n            type: \"success\",\n            duration: 5000\n          });\n        }\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"新增公民失败！\",\n          type: \"error\",\n          duration: 5000\n        });\n      }\n    },\n    async delCitizenInfo() {\n      try {\n        const response = await axios.delete(`/api/delCitizen?citizenID=${encodeURIComponent(this.delCitizenID)}`);\n        if (typeof response.data == \"string\") {\n          ElMessage({\n            showClose: true,\n            message: response.data,\n            type: \"warning\",\n            duration: 5000\n          });\n        } else {\n          await this.fetchCitizenInfo();\n          ElMessage({\n            showClose: true,\n            message: \"删除公民成功! \",\n            type: \"success\",\n            duration: 5000\n          });\n        }\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"删除公民失败！\",\n          type: \"error\",\n          duration: 5000\n        });\n      }\n    },\n    async updCitizenInfo() {\n      try {\n        const response = await axios.put(`/api/updCitizen?citizenID=${encodeURIComponent(this.updCitizenID)}&citizenName=${encodeURIComponent(this.updCitizenName)}&citizenSex=${encodeURIComponent(this.updCitizenSex)}&citizenAddress=${encodeURIComponent(this.updCitizenAddress)}&motherID=${encodeURIComponent(this.updMotherCitizenID)}&fatherID=${encodeURIComponent(this.updFatherCitizenID)}`);\n        if (typeof response.data == \"string\") {\n          ElMessage({\n            showClose: true,\n            message: response.data,\n            type: \"warning\",\n            duration: 5000\n          });\n        } else {\n          await this.fetchCitizenInfo();\n          ElMessage({\n            showClose: true,\n            message: \"修改成功! \",\n            type: \"success\",\n            duration: 5000\n          });\n        }\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"修改失败！\",\n          type: \"error\",\n          duration: 5000\n        });\n      }\n    },\n    async back() {\n      this.citizenUseInfo = \"\";\n      this.showCitizen = true;\n    }\n  }\n};","map":{"version":3,"names":["axios","ElMessage","data","anv","citizenID","citizenName","citizenAddress","citizenSex","citizenInfo","addCitizenID","addCitizenName","addCitizenAddress","addCitizenSex","addMotherCitizenID","addFatherCitizenID","delCitizenID","updCitizenID","updCitizenName","updCitizenAddress","updCitizenSex","updMotherCitizenID","updFatherCitizenID","methods","handleMouseMove","event","x","pageX","target","offsetLeft","y","pageY","offsetTop","style","setProperty","fetchCitizenInfo","response","get","encodeURIComponent","map","item","sex","error","showClose","message","type","duration","addCitizenInfo","post","delCitizenInfo","delete","updCitizenInfo","put","back","citizenUseInfo","showCitizen"],"sources":["H:\\GitHub-Repository\\2023-TJU-DBD-Police-Affair-System\\My-front\\dbproject\\src\\components\\UrbanResidentmanagement\\CitizenInfoManagement.vue"],"sourcesContent":["<template>\n  <el-header class=\"sub-header\" @mousemove=\"handleMouseMove\">\n    <div>&nbsp;&nbsp;城区和居民管理&nbsp;>&nbsp;公民户籍管理</div>\n  </el-header>\n\n  <el-tabs v-model=\"anv\" type=\"border-card\" style=\"margin-top: 10vh\">\n    <!-- 查询栏 -->\n    <el-tab-pane label=\"查询公民信息\" name=\"1\">\n      <div style=\"text-align: center\">\n        <table style=\"display: inline-block; text-align: left\">\n          <tr>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">身份证号:</el-text>\n              <el-input\n                class=\"inputBox\"\n                v-model=\"citizenID\"\n                placeholder=\"请输入身份证号\"\n                clearable\n                maxlength=\"18\"\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\n                style=\"width: 235px\"\n                show-word-limit\n              />\n            </td>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">姓名:</el-text>\n              <el-input\n                class=\"inputBox\"\n                v-model=\"citizenName\"\n                placeholder=\"请输入姓名\"\n                clearable\n              />\n            </td>\n          </tr>\n          <tr>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">性别:</el-text>\n              <el-select\n                class=\"inputBox\"\n                v-model=\"citizenSex\"\n                style=\"width: 235px\"\n                placeholder=\"请选择\"\n              >\n                <el-option label=\"全部性别\" selected value=\"\"></el-option>\n                <el-option label=\"男\" value=\"M\"></el-option>\n                <el-option label=\"女\" value=\"F\"></el-option>\n              </el-select>\n            </td>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">住址:</el-text>\n              <el-input\n                class=\"inputBox\"\n                v-model=\"citizenAddress\"\n                placeholder=\"请输入地址\"\n                clearable\n              />\n            </td>\n          </tr>\n        </table>\n\n        <div class=\"seaButton\">\n          <el-button type=\"primary\" @click=\"fetchCitizenInfo\">查询</el-button>\n        </div>\n      </div>\n\n      <el-divider />\n\n      <div>\n        <!-- 表格显示获取的公民信息 -->\n        <el-table :data=\"citizenInfo\" stripe height=\"450\" @wheel.passive.stop>\n          <el-table-column prop=\"id\" label=\"身份证号\" sortable />\n          <el-table-column\n            prop=\"name\"\n            label=\"公民名称\"\n            sortable\n            width=\"120px\"\n          />\n          <el-table-column prop=\"sex\" label=\"性别\" sortable width=\"80px\" />\n          <el-table-column prop=\"address\" label=\"住址\" sortable />\n          <el-table-column prop=\"motherid\" label=\"母亲身份证号\" sortable />\n          <el-table-column prop=\"fatherid\" label=\"父亲身份证号\" sortable />\n        </el-table>\n      </div>\n    </el-tab-pane>\n\n    <!-- 新增栏 -->\n    <el-tab-pane label=\"公民信息登记\" name=\"2\">\n      <div style=\"text-align: center\">\n        <table style=\"display: inline-block; text-align: left\">\n          <tr>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">身份证号:</el-text>\n              <el-input\n                class=\"inputBox\"\n                v-model=\"addCitizenID\"\n                placeholder=\"请输入身份证号\"\n                clearable\n                maxlength=\"18\"\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\n                style=\"width: 235px\"\n                show-word-limit\n              />\n            </td>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">姓名:</el-text>\n              <el-input\n                class=\"inputBox\"\n                v-model=\"addCitizenName\"\n                placeholder=\"请输入姓名\"\n                clearable\n              />\n            </td>\n          </tr>\n          <tr>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">母亲身份证号:</el-text>\n              <el-input\n                class=\"inputBox\"\n                v-model=\"addMotherCitizenID\"\n                placeholder=\"请输入身份证号\"\n                clearable\n                maxlength=\"18\"\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\n                style=\"width: 235px\"\n                show-word-limit\n              />\n            </td>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">住址:</el-text>\n              <el-input\n                class=\"inputBox\"\n                v-model=\"addCitizenAddress\"\n                placeholder=\"请输入地址\"\n                clearable\n              />\n            </td>\n          </tr>\n          <tr>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">父亲身份证号:</el-text>\n              <el-input\n                class=\"inputBox\"\n                v-model=\"addFatherCitizenID\"\n                placeholder=\"请输入身份证号\"\n                clearable\n                maxlength=\"18\"\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\n                style=\"width: 235px\"\n                show-word-limit\n              />\n            </td>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">性别:</el-text>\n              <el-select\n                class=\"inputBox\"\n                v-model=\"addCitizenSex\"\n                placeholder=\"请选择\"\n              >\n                <el-option label=\"男\" value=\"M\"></el-option>\n                <el-option label=\"女\" value=\"F\"></el-option>\n              </el-select>\n            </td>\n          </tr>\n        </table>\n\n        <div class=\"seaButton\">\n          <el-button type=\"primary\" @click=\"addCitizenInfo\"\n            >上传公民信息</el-button\n          >\n        </div>\n      </div>\n    </el-tab-pane>\n\n    <!-- 删除栏 -->\n    <el-tab-pane label=\"删除公民信息\" name=\"3\">\n      <div style=\"text-align: center\">\n        <table style=\"display: inline-block; text-align: left\">\n          <tr>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">身份证号：</el-text>\n              <el-input\n                class=\"inputBox\"\n                v-model=\"delCitizenID\"\n                placeholder=\"请输入身份证号\"\n                clearable\n                maxlength=\"18\"\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\n                style=\"width: 250px\"\n                show-word-limit\n              />\n            </td>\n            <td>\n              <el-button\n                type=\"primary\"\n                @click=\"delCitizenInfo\"\n                style=\"margin-left: 20px\"\n                >删除公民信息</el-button\n              >\n            </td>\n          </tr>\n        </table>\n      </div>\n    </el-tab-pane>\n\n    <!-- 修改栏 -->\n    <el-tab-pane label=\"修改公民信息\" name=\"4\">\n      <div style=\"text-align: center\">\n        <table style=\"display: inline-block; text-align: left\">\n          <tr>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">身份证号:</el-text>\n              <el-input\n                class=\"inputBox\"\n                v-model=\"updCitizenID\"\n                placeholder=\"请输入身份证号\"\n                clearable\n                maxlength=\"18\"\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\n                style=\"width: 235px\"\n                show-word-limit\n              />\n            </td>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">更新姓名:</el-text>\n              <el-input\n                class=\"inputBox\"\n                v-model=\"updCitizenName\"\n                placeholder=\"请输入姓名\"\n                clearable\n              />\n            </td>\n          </tr>\n          <tr>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\"\n                >更新母亲身份证号:</el-text\n              >\n              <el-input\n                class=\"inputBox\"\n                v-model=\"updMotherCitizenID\"\n                placeholder=\"请输入身份证号\"\n                clearable\n                maxlength=\"18\"\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\n                style=\"width: 235px\"\n                show-word-limit\n              />\n            </td>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">更新住址:</el-text>\n              <el-input\n                class=\"inputBox\"\n                v-model=\"updCitizenAddress\"\n                placeholder=\"请输入地址\"\n                clearable\n              />\n            </td>\n          </tr>\n          <tr>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\"\n                >更新父亲身份证号:</el-text\n              >\n              <el-input\n                class=\"inputBox\"\n                v-model=\"updFatherCitizenID\"\n                placeholder=\"请输入身份证号\"\n                clearable\n                maxlength=\"18\"\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\n                style=\"width: 235px\"\n                show-word-limit\n              />\n            </td>\n            <td>\n              <el-text class=\"noteText\" type=\"primary\">更新性别:</el-text>\n              <el-select\n                class=\"inputBox\"\n                v-model=\"updCitizenSex\"\n                placeholder=\"请选择\"\n              >\n                <el-option label=\"男\" value=\"M\"></el-option>\n                <el-option label=\"女\" value=\"F\"></el-option>\n              </el-select>\n            </td>\n          </tr>\n        </table>\n\n        <div class=\"seaButton\">\n          <el-button type=\"primary\" @click=\"updCitizenInfo\"\n            >更新公民信息</el-button\n          >\n        </div>\n      </div>\n    </el-tab-pane>\n  </el-tabs>\n</template>\n\n<script>\nimport axios from \"../../api/request\";\n\nimport { ElMessage } from \"element-plus\";\n\nexport default {\n  data() {\n    return {\n      anv: \"1\",\n      citizenID: \"\",\n      citizenName: \"\",\n      citizenAddress: \"\",\n      citizenSex: \"\",\n      citizenInfo: [],\n      addCitizenID: \"\",\n      addCitizenName: \"\",\n      addCitizenAddress: \"\",\n      addCitizenSex: \"\",\n      addMotherCitizenID: \"\",\n      addFatherCitizenID: \"\",\n      delCitizenID: \"\",\n      updCitizenID: \"\",\n      updCitizenName: \"\",\n      updCitizenAddress: \"\",\n      updCitizenSex: \"\",\n      updMotherCitizenID: \"\",\n      updFatherCitizenID: \"\",\n    };\n  },\n  methods: {\n    handleMouseMove(event) {\n      const x = event.pageX - event.target.offsetLeft;\n      const y = event.pageY - event.target.offsetTop;\n\n      event.target.style.setProperty(\"--x\", `${x}px`);\n      event.target.style.setProperty(\"--y\", `${y}px`);\n    },\n    async fetchCitizenInfo() {\n      try {\n        const response = await axios.get(\n          `/api/getCitizenInfo?citizenID=${encodeURIComponent(\n            this.citizenID\n          )}&citizenName=${encodeURIComponent(\n            this.citizenName\n          )}&citizenSex=${encodeURIComponent(\n            this.citizenSex\n          )}&citizenAddress=${encodeURIComponent(this.citizenAddress)}`\n        );\n        this.citizenInfo = response.data.map((item) => {\n          if (item.sex === \"M\") {\n            return { ...item, sex: \"男\" };\n          } else if (item.sex === \"F\") {\n            return { ...item, sex: \"女\" };\n          } else {\n            return item;\n          }\n        });\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"获取公民列表失败！\",\n          type: \"error\",\n          duration: 5000,\n        });\n      }\n    },\n    async addCitizenInfo() {\n      try {\n        const response = await axios.post(\n          `/api/addCitizen?citizenID=${encodeURIComponent(\n            this.addCitizenID\n          )}&citizenName=${encodeURIComponent(\n            this.addCitizenName\n          )}&citizenSex=${encodeURIComponent(\n            this.addCitizenSex\n          )}&citizenAddress=${encodeURIComponent(\n            this.addCitizenAddress\n          )}&motherID=${encodeURIComponent(\n            this.addMotherCitizenID\n          )}&fatherID=${encodeURIComponent(this.addFatherCitizenID)}`\n        );\n        if (typeof response.data == \"string\") {\n          ElMessage({\n            showClose: true,\n            message: response.data,\n            type: \"warning\",\n            duration: 5000,\n          });\n        } else {\n          await this.fetchCitizenInfo();\n          ElMessage({\n            showClose: true,\n            message: \"新增公民成功! \",\n            type: \"success\",\n            duration: 5000,\n          });\n        }\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"新增公民失败！\",\n          type: \"error\",\n          duration: 5000,\n        });\n      }\n    },\n    async delCitizenInfo() {\n      try {\n        const response = await axios.delete(\n          `/api/delCitizen?citizenID=${encodeURIComponent(this.delCitizenID)}`\n        );\n        if (typeof response.data == \"string\") {\n          ElMessage({\n            showClose: true,\n            message: response.data,\n            type: \"warning\",\n            duration: 5000,\n          });\n        } else {\n          await this.fetchCitizenInfo();\n          ElMessage({\n            showClose: true,\n            message: \"删除公民成功! \",\n            type: \"success\",\n            duration: 5000,\n          });\n        }\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"删除公民失败！\",\n          type: \"error\",\n          duration: 5000,\n        });\n      }\n    },\n    async updCitizenInfo() {\n      try {\n        const response = await axios.put(\n          `/api/updCitizen?citizenID=${encodeURIComponent(\n            this.updCitizenID\n          )}&citizenName=${encodeURIComponent(\n            this.updCitizenName\n          )}&citizenSex=${encodeURIComponent(\n            this.updCitizenSex\n          )}&citizenAddress=${encodeURIComponent(\n            this.updCitizenAddress\n          )}&motherID=${encodeURIComponent(\n            this.updMotherCitizenID\n          )}&fatherID=${encodeURIComponent(this.updFatherCitizenID)}`\n        );\n        if (typeof response.data == \"string\") {\n          ElMessage({\n            showClose: true,\n            message: response.data,\n            type: \"warning\",\n            duration: 5000,\n          });\n        } else {\n          await this.fetchCitizenInfo();\n          ElMessage({\n            showClose: true,\n            message: \"修改成功! \",\n            type: \"success\",\n            duration: 5000,\n          });\n        }\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"修改失败！\",\n          type: \"error\",\n          duration: 5000,\n        });\n      }\n    },\n    async back() {\n      this.citizenUseInfo = \"\";\n      this.showCitizen = true;\n    },\n  },\n};\n</script>\n\n<style lang=\"postcss\" scoped>\n.sub-header {\n  overflow: hidden;\n  display: flex;\n  position: absolute;\n  top: 70px;\n  left: 199px;\n  width: calc(100% - 199px);\n  height: 7vh;\n  min-height: 40px;\n  align-items: center;\n  /* 文字竖直方向居中对齐 */\n  background-color: #1f2cdf;\n  box-shadow: inset -500px 0px 200px 0px rgba(4, 0, 113, 0.856);\n  color: #ffffff;\n  font-size: 28px;\n}\n\n.sub-header::before {\n  --size: 0;\n  content: \"\";\n  position: absolute;\n  left: var(--x);\n  top: var(--y);\n  width: var(--size);\n  height: var(--size);\n  background: radial-gradient(circle closest-side, #5a65ff, transparent);\n  transform: translate(-50%, -50%);\n  transition: width 0.2s ease, height 0.2s ease;\n}\n\n.