{"ast":null,"code":"import axios from \"../../api/request\";\nimport { ElMessage } from \"element-plus\";\nexport default {\n  data() {\n    return {\n      anv: \"1\",\n      citizenID: \"\",\n      citizenName: \"\",\n      citizenAddress: \"\",\n      citizenSex: \"\",\n      citizenInfo: [],\n      addCitizenID: \"\",\n      addCitizenName: \"\",\n      addCitizenAddress: \"\",\n      addCitizenSex: \"\",\n      addMotherCitizenID: \"\",\n      addFatherCitizenID: \"\",\n      delCitizenID: \"\",\n      updCitizenID: \"\",\n      updCitizenName: \"\",\n      updCitizenAddress: \"\",\n      updCitizenSex: \"\",\n      updMotherCitizenID: \"\",\n      updFatherCitizenID: \"\"\n    };\n  },\n  methods: {\n    handleMouseMove(event) {\n      const x = event.pageX - event.target.offsetLeft;\n      const y = event.pageY - event.target.offsetTop;\n      event.target.style.setProperty(\"--x\", `${x}px`);\n      event.target.style.setProperty(\"--y\", `${y}px`);\n    },\n    async fetchCitizenInfo() {\n      try {\n        const response = await axios.get(`/api/getCitizenInfo?citizenID=${encodeURIComponent(this.citizenID)}&citizenName=${encodeURIComponent(this.citizenName)}&citizenSex=${encodeURIComponent(this.citizenSex)}&citizenAddress=${encodeURIComponent(this.citizenAddress)}`);\n        this.citizenInfo = response.data.map(item => {\n          if (item.sex === \"M\") {\n            return {\n              ...item,\n              sex: \"男\"\n            };\n          } else if (item.sex === \"F\") {\n            return {\n              ...item,\n              sex: \"女\"\n            };\n          } else {\n            return item;\n          }\n        });\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"获取公民列表失败！\",\n          type: \"error\",\n          duration: 5000\n        });\n      }\n    },\n    async addCitizenInfo() {\n      try {\n        const response = await axios.post(`/api/addCitizen?citizenID=${encodeURIComponent(this.addCitizenID)}&citizenName=${encodeURIComponent(this.addCitizenName)}&citizenSex=${encodeURIComponent(this.addCitizenSex)}&citizenAddress=${encodeURIComponent(this.addCitizenAddress)}&motherID=${encodeURIComponent(this.addMotherCitizenID)}&fatherID=${encodeURIComponent(this.addFatherCitizenID)}`);\n        if (typeof response.data == \"string\") {\n          ElMessage({\n            showClose: true,\n            message: response.data,\n            type: \"warning\",\n            duration: 5000\n          });\n        } else {\n          await this.fetchCitizenInfo();\n          ElMessage({\n            showClose: true,\n            message: \"新增公民成功! \",\n            type: \"success\",\n            duration: 5000\n          });\n        }\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"新增公民失败！\",\n          type: \"error\",\n          duration: 5000\n        });\n      }\n    },\n    async delCitizenInfo() {\n      try {\n        const response = await axios.delete(`/api/delCitizen?citizenID=${encodeURIComponent(this.delCitizenID)}`);\n        if (typeof response.data == \"string\") {\n          ElMessage({\n            showClose: true,\n            message: response.data,\n            type: \"warning\",\n            duration: 5000\n          });\n        } else {\n          await this.fetchCitizenInfo();\n          ElMessage({\n            showClose: true,\n            message: \"删除公民成功! \",\n            type: \"success\",\n            duration: 5000\n          });\n        }\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"删除公民失败！\",\n          type: \"error\",\n          duration: 5000\n        });\n      }\n    },\n    async updCitizenInfo() {\n      try {\n        const response = await axios.put(`/api/updCitizen?citizenID=${encodeURIComponent(this.updCitizenID)}&citizenName=${encodeURIComponent(this.updCitizenName)}&citizenSex=${encodeURIComponent(this.updCitizenSex)}&citizenAddress=${encodeURIComponent(this.updCitizenAddress)}&motherID=${encodeURIComponent(this.updMotherCitizenID)}&fatherID=${encodeURIComponent(this.updFatherCitizenID)}`);\n        if (typeof response.data == \"string\") {\n          ElMessage({\n            showClose: true,\n            message: response.data,\n            type: \"warning\",\n            duration: 5000\n          });\n        } else {\n          await this.fetchCitizenInfo();\n          ElMessage({\n            showClose: true,\n            message: \"修改成功! \",\n            type: \"success\",\n            duration: 5000\n          });\n        }\n      } catch (error) {\n        // 请求失败时的处理逻辑\n        ElMessage({\n          showClose: true,\n          message: \"修改失败！\",\n          type: \"error\",\n          duration: 5000\n        });\n      }\n    },\n    async back() {\n      this.citizenUseInfo = \"\";\n      this.showCitizen = true;\n    }\n  }\n};","map":{"version":3,"names":["axios","ElMessage","data","anv","citizenID","citizenName","citizenAddress","citizenSex","citizenInfo","addCitizenID","addCitizenName","addCitizenAddress","addCitizenSex","addMotherCitizenID","addFatherCitizenID","delCitizenID","updCitizenID","updCitizenName","updCitizenAddress","updCitizenSex","updMotherCitizenID","updFatherCitizenID","methods","handleMouseMove","event","x","pageX","target","offsetLeft","y","pageY","offsetTop","style","setProperty","fetchCitizenInfo","response","get","encodeURIComponent","map","item","sex","error","showClose","message","type","duration","addCitizenInfo","post","delCitizenInfo","delete","updCitizenInfo","put","back","citizenUseInfo","showCitizen"],"sources":["H:\\GitHub-Repository\\2023-TJU-DBD-Police-Affair-System\\My-front\\dbproject\\src\\components\\UrbanResidentmanagement\\CitizenInfoManagement.vue"],"sourcesContent":["<template>\r\n  <el-header class=\"sub-header\" @mousemove=\"handleMouseMove\">\r\n    <div>&nbsp;&nbsp;城区和居民管理&nbsp;>&nbsp;公民户籍管理</div>\r\n  </el-header>\r\n\r\n  <el-tabs v-model=\"anv\" type=\"border-card\" style=\"margin-top: 10vh\">\r\n    <!-- 查询栏 -->\r\n    <el-tab-pane label=\"查询公民信息\" name=\"1\">\r\n      <div style=\"text-align: center\">\r\n        <table style=\"display: inline-block; text-align: left\">\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">身份证号:</el-text>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"citizenID\"\r\n                placeholder=\"请输入身份证号\"\r\n                clearable\r\n                maxlength=\"18\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\r\n                style=\"width: 250px\"\r\n                show-word-limit\r\n              />\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">姓名:</el-text>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"citizenName\"\r\n                placeholder=\"请输入姓名\"\r\n                clearable\r\n              />\r\n            </td>\r\n          </tr>\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">性别:</el-text>\r\n              <el-select\r\n                class=\"inputBox\"\r\n                v-model=\"citizenSex\"\r\n                style=\"width: 231px\"\r\n                placeholder=\"请选择\"\r\n              >\r\n                <el-option label=\"全部性别\" selected value=\"\"></el-option>\r\n                <el-option label=\"男\" value=\"M\"></el-option>\r\n                <el-option label=\"女\" value=\"F\"></el-option>\r\n              </el-select>\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">住址:</el-text>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"citizenAddress\"\r\n                placeholder=\"请输入地址\"\r\n                clearable\r\n              />\r\n            </td>\r\n          </tr>\r\n        </table>\r\n\r\n        <div class=\"seaButton\">\r\n          <el-button type=\"primary\" @click=\"fetchCitizenInfo\">查询</el-button>\r\n        </div>\r\n      </div>\r\n\r\n      <el-divider />\r\n\r\n      <div>\r\n        <!-- 表格显示获取的公民信息 -->\r\n        <el-table :data=\"citizenInfo\" stripe height=\"450\" @wheel.passive.stop>\r\n          <el-table-column prop=\"id\" label=\"身份证号\" sortable />\r\n          <el-table-column\r\n            prop=\"name\"\r\n            label=\"公民名称\"\r\n            sortable\r\n            width=\"120px\"\r\n          />\r\n          <el-table-column prop=\"sex\" label=\"性别\" sortable width=\"80px\" />\r\n          <el-table-column prop=\"address\" label=\"住址\" sortable />\r\n          <el-table-column prop=\"motherid\" label=\"母亲身份证号\" sortable />\r\n          <el-table-column prop=\"fatherid\" label=\"父亲身份证号\" sortable />\r\n        </el-table>\r\n      </div>\r\n    </el-tab-pane>\r\n\r\n    <!-- 新增栏 -->\r\n    <el-tab-pane label=\"公民信息登记\" name=\"2\">\r\n      <div style=\"text-align: center\">\r\n        <table style=\"display: inline-block; text-align: left\">\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">身份证号:</el-text>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"addCitizenID\"\r\n                placeholder=\"请输入身份证号\"\r\n                clearable\r\n                maxlength=\"18\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\r\n                style=\"width: 250px\"\r\n                show-word-limit\r\n              />\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">姓名:</el-text>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"addCitizenName\"\r\n                placeholder=\"请输入姓名\"\r\n                clearable\r\n              />\r\n            </td>\r\n          </tr>\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">母亲身份证号:</el-text>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"addMotherCitizenID\"\r\n                placeholder=\"请输入身份证号\"\r\n                clearable\r\n                maxlength=\"18\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\r\n                style=\"width: 250px\"\r\n                show-word-limit\r\n              />\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">住址:</el-text>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"addCitizenAddress\"\r\n                placeholder=\"请输入地址\"\r\n                clearable\r\n              />\r\n            </td>\r\n          </tr>\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">父亲身份证号:</el-text>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"addFatherCitizenID\"\r\n                placeholder=\"请输入身份证号\"\r\n                clearable\r\n                maxlength=\"18\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\r\n                style=\"width: 250px\"\r\n                show-word-limit\r\n              />\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">性别:</el-text>\r\n              <el-select\r\n                class=\"inputBox\"\r\n                v-model=\"addCitizenSex\"\r\n                placeholder=\"请选择\"\r\n                style=\"width: 181px\"\r\n              >\r\n                <el-option label=\"男\" value=\"M\"></el-option>\r\n                <el-option label=\"女\" value=\"F\"></el-option>\r\n              </el-select>\r\n            </td>\r\n          </tr>\r\n        </table>\r\n\r\n        <div class=\"seaButton\">\r\n          <el-button type=\"primary\" @click=\"addCitizenInfo\"\r\n            >登记公民信息</el-button\r\n          >\r\n        </div>\r\n      </div>\r\n    </el-tab-pane>\r\n\r\n    <!-- 删除栏 -->\r\n    <el-tab-pane label=\"删除公民信息\" name=\"3\">\r\n      <div style=\"text-align: center\">\r\n        <table style=\"display: inline-block; text-align: left\">\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">身份证号：</el-text>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"delCitizenID\"\r\n                placeholder=\"请输入身份证号\"\r\n                clearable\r\n                maxlength=\"18\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\r\n                style=\"width: 250px\"\r\n                show-word-limit\r\n              />\r\n            </td>\r\n            <td>\r\n              <el-button\r\n                type=\"primary\"\r\n                @click=\"delCitizenInfo\"\r\n                style=\"margin-left: 20px\"\r\n                >删除公民信息</el-button\r\n              >\r\n            </td>\r\n          </tr>\r\n        </table>\r\n      </div>\r\n    </el-tab-pane>\r\n\r\n    <!-- 修改栏 -->\r\n    <el-tab-pane label=\"修改公民信息\" name=\"4\">\r\n      <div style=\"text-align: center\">\r\n        <table style=\"display: inline-block; text-align: left\">\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">身份证号:</el-text>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"updCitizenID\"\r\n                placeholder=\"请输入身份证号\"\r\n                clearable\r\n                maxlength=\"18\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\r\n                style=\"width: 250px\"\r\n                show-word-limit\r\n              />\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">更新姓名:</el-text>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"updCitizenName\"\r\n                placeholder=\"请输入姓名\"\r\n                clearable\r\n              />\r\n            </td>\r\n          </tr>\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\"\r\n                >更新母亲身份证号:</el-text\r\n              >\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"updMotherCitizenID\"\r\n                placeholder=\"请输入身份证号\"\r\n                clearable\r\n                maxlength=\"18\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\r\n                style=\"width: 250px\"\r\n                show-word-limit\r\n              />\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">更新住址:</el-text>\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"updCitizenAddress\"\r\n                placeholder=\"请输入地址\"\r\n                clearable\r\n              />\r\n            </td>\r\n          </tr>\r\n          <tr>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\"\r\n                >更新父亲身份证号:</el-text\r\n              >\r\n              <el-input\r\n                class=\"inputBox\"\r\n                v-model=\"updFatherCitizenID\"\r\n                placeholder=\"请输入身份证号\"\r\n                clearable\r\n                maxlength=\"18\"\r\n                oninput=\"value=value.replace(/[^\\d.]/g,'')\"\r\n                style=\"width: 250px\"\r\n                show-word-limit\r\n              />\r\n            </td>\r\n            <td>\r\n              <el-text class=\"noteText\" type=\"primary\">更新性别:</el-text>\r\n              <el-select\r\n                class=\"inputBox\"\r\n                v-model=\"updCitizenSex\"\r\n                placeholder=\"请选择\"\r\n                style=\"width: 181px\"\r\n              >\r\n                <el-option label=\"男\" value=\"M\"></el-option>\r\n                <el-option label=\"女\" value=\"F\"></el-option>\r\n              </el-select>\r\n            </td>\r\n          </tr>\r\n        </table>\r\n\r\n        <div class=\"seaButton\">\r\n          <el-button type=\"primary\" @click=\"updCitizenInfo\"\r\n            >提交新的信息</el-button\r\n          >\r\n        </div>\r\n      </div>\r\n    </el-tab-pane>\r\n  </el-tabs>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"../../api/request\";\r\n\r\nimport { ElMessage } from \"element-plus\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      anv: \"1\",\r\n      citizenID: \"\",\r\n      citizenName: \"\",\r\n      citizenAddress: \"\",\r\n      citizenSex: \"\",\r\n      citizenInfo: [],\r\n      addCitizenID: \"\",\r\n      addCitizenName: \"\",\r\n      addCitizenAddress: \"\",\r\n      addCitizenSex: \"\",\r\n      addMotherCitizenID: \"\",\r\n      addFatherCitizenID: \"\",\r\n      delCitizenID: \"\",\r\n      updCitizenID: \"\",\r\n      updCitizenName: \"\",\r\n      updCitizenAddress: \"\",\r\n      updCitizenSex: \"\",\r\n      updMotherCitizenID: \"\",\r\n      updFatherCitizenID: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    handleMouseMove(event) {\r\n      const x = event.pageX - event.target.offsetLeft;\r\n      const y = event.pageY - event.target.offsetTop;\r\n\r\n      event.target.style.setProperty(\"--x\", `${x}px`);\r\n      event.target.style.setProperty(\"--y\", `${y}px`);\r\n    },\r\n    async fetchCitizenInfo() {\r\n      try {\r\n        const response = await axios.get(\r\n          `/api/getCitizenInfo?citizenID=${encodeURIComponent(\r\n            this.citizenID\r\n          )}&citizenName=${encodeURIComponent(\r\n            this.citizenName\r\n          )}&citizenSex=${encodeURIComponent(\r\n            this.citizenSex\r\n          )}&citizenAddress=${encodeURIComponent(this.citizenAddress)}`\r\n        );\r\n        this.citizenInfo = response.data.map((item) => {\r\n          if (item.sex === \"M\") {\r\n            return { ...item, sex: \"男\" };\r\n          } else if (item.sex === \"F\") {\r\n            return { ...item, sex: \"女\" };\r\n          } else {\r\n            return item;\r\n          }\r\n        });\r\n      } catch (error) {\r\n        // 请求失败时的处理逻辑\r\n        ElMessage({\r\n          showClose: true,\r\n          message: \"获取公民列表失败！\",\r\n          type: \"error\",\r\n          duration: 5000,\r\n        });\r\n      }\r\n    },\r\n    async addCitizenInfo() {\r\n      try {\r\n        const response = await axios.post(\r\n          `/api/addCitizen?citizenID=${encodeURIComponent(\r\n            this.addCitizenID\r\n          )}&citizenName=${encodeURIComponent(\r\n            this.addCitizenName\r\n          )}&citizenSex=${encodeURIComponent(\r\n            this.addCitizenSex\r\n          )}&citizenAddress=${encodeURIComponent(\r\n            this.addCitizenAddress\r\n          )}&motherID=${encodeURIComponent(\r\n            this.addMotherCitizenID\r\n          )}&fatherID=${encodeURIComponent(this.addFatherCitizenID)}`\r\n        );\r\n        if (typeof response.data == \"string\") {\r\n          ElMessage({\r\n            showClose: true,\r\n            message: response.data,\r\n            type: \"warning\",\r\n            duration: 5000,\r\n          });\r\n        } else {\r\n          await this.fetchCitizenInfo();\r\n          ElMessage({\r\n            showClose: true,\r\n            message: \"新增公民成功! \",\r\n            type: \"success\",\r\n            duration: 5000,\r\n          });\r\n        }\r\n      } catch (error) {\r\n        // 请求失败时的处理逻辑\r\n        ElMessage({\r\n          showClose: true,\r\n          message: \"新增公民失败！\",\r\n          type: \"error\",\r\n          duration: 5000,\r\n        });\r\n      }\r\n    },\r\n    async delCitizenInfo() {\r\n      try {\r\n        const response = await axios.delete(\r\n          `/api/delCitizen?citizenID=${encodeURIComponent(this.delCitizenID)}`\r\n        );\r\n        if (typeof response.data == \"string\") {\r\n          ElMessage({\r\n            showClose: true,\r\n            message: response.data,\r\n            type: \"warning\",\r\n            duration: 5000,\r\n          });\r\n        } else {\r\n          await this.fetchCitizenInfo();\r\n          ElMessage({\r\n            showClose: true,\r\n            message: \"删除公民成功! \",\r\n            type: \"success\",\r\n            duration: 5000,\r\n          });\r\n        }\r\n      } catch (error) {\r\n        // 请求失败时的处理逻辑\r\n        ElMessage({\r\n          showClose: true,\r\n          message: \"删除公民失败！\",\r\n          type: \"error\",\r\n          duration: 5000,\r\n        });\r\n      }\r\n    },\r\n    async updCitizenInfo() {\r\n      try {\r\n        const response = await axios.put(\r\n          `/api/updCitizen?citizenID=${encodeURIComponent(\r\n            this.updCitizenID\r\n          )}&citizenName=${encodeURIComponent(\r\n            this.updCitizenName\r\n          )}&citizenSex=${encodeURIComponent(\r\n            this.updCitizenSex\r\n          )}&citizenAddress=${encodeURIComponent(\r\n            this.updCitizenAddress\r\n          )}&motherID=${encodeURIComponent(\r\n            this.updMotherCitizenID\r\n          )}&fatherID=${encodeURIComponent(this.updFatherCitizenID)}`\r\n        );\r\n        if (typeof response.data == \"string\") {\r\n          ElMessage({\r\n            showClose: true,\r\n            message: response.data,\r\n            type: \"warning\",\r\n            duration: 5000,\r\n          });\r\n        } else {\r\n          await this.fetchCitizenInfo();\r\n          ElMessage({\r\n            showClose: true,\r\n            message: \"修改成功! \",\r\n            type: \"success\",\r\n            duration: 5000,\r\n          });\r\n        }\r\n      } catch (error) {\r\n     